  "phone_number": "555888",
  "phone_country_code": "",
  "dob": "1994-06-16",
  "gender": "Male",
  "locale": "en"
}



### Logout
POST {{host}}/{{locale}}/spa/logout
Accept: application/json
Content-Type: application/json
Authorization: Bearer {{token}}

